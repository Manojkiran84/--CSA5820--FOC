#include<stdio.h>
#include<conio.h>
int main()
{
int n1,n2, n3, temp;
printf("Enter n1: "); 
scanf("%d",&n1);
printf("Enter n2: "); 
scanf("%d",&n2);
printf("Enter n3: "); 
scanf("%d",&n3);
printf("Before swapping \n");
printf("n1: %d \n", n1);
printf("n2: %d \n", n2);
printf("n3: %d \n", n3);
temp = n1;  
n1 = n2; 
n2= n3; 
n3 = temp; 
printf("After swapping \n");
printf("n1: %d \n", n1);
printf("n2: %d \n", n2);
printf("n3: %d \n", n3);
return 0;
}
